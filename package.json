{
  "name": "@api3/operations",
  "license": "MIT",
  "private": false,
  "version": "0.0.1-d",
  "description": "A package to parse API3 operations metadata",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": "^16.17.0"
  },
  "files": [
    "dist",
    "data",
    "chain"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "create-boilerplate": "ts-node src/deployment/create-boilerplate.ts",
    "create-airseeker-config": "ts-node src/deployment/create-airseeker-config.ts",
    "export-ops-json-payloads": "ts-node src/utils/export-ops-json-payloads.ts",
    "eth:deploy": "cd chain && hardhat deploy --network $NETWORK",
    "deploy-all": "yarn deploy-airnode; yarn deploy-airkeeper",
    "deploy-airkeeper": "yarn ts-node src/deployment/deploy-airkeeper",
    "generate-topup-wallets": "ts-node src/utils/generate-topup-wallets.ts",
    "register-subscriptions": "ts-node src/register/register-subscriptions.ts",
    "add-missing-beacon-metadata": "ts-node src/market/add-missing-beacon-metadata.ts",
    "build": "yarn run clean && yarn run compile",
    "check-subscriptions": "ts-node src/inspect/check-subscriptions.ts",
    "check-dapi-name": "ts-node src/inspect/check-dapi-name.ts",
    "check-policy-readers": "ts-node src/inspect/check-policy-readers.ts",
    "enable-policy-readers": "ts-node src/register/enable-policy-readers.ts",
    "clean": "rimraf -rf ./dist *.tgz",
    "compile": "tsc -p tsconfig.json",
    "normalize-and-validate": "ts-node src/validation/normalize-and-validate.ts",
    "validate": "ts-node src/validation/validate.ts",
    "normalize": "ts-node src/validation/normalize.ts",
    "lint": "yarn prettier:check && yarn lint:eslint && yarn lint:prisma-format",
    "lint:prisma-format": "yarn prisma format",
    "lint:eslint": "eslint . --ext .js,.ts",
    "prisma:studio": "yarn prisma studio",
    "prisma:generate": "yarn prisma generate",
    "database:dev:start": "docker run --name pg-ops-dev -d --rm -p 127.0.0.1:5432:5432 -e POSTGRES_PASSWORD=password postgres:14",
    "database:dev:stop": "docker kill pg-ops-dev",
    "database:translate-operations": "yarn ts-node src/database/translate-operations.ts",
    "prettier:check": "prettier --check \"./**/*.{js,ts,md,json}\"",
    "prettier:write": "prettier --write \"./**/*.{js,ts,md,json}\"",
    "test": "jest --verbose --runInBand --bail --detectOpenHandles",
    "pack": "yarn pack",
    "start": "ts-node src/index.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/api3dao/operations.git"
  },
  "devDependencies": {
    "@api3/airnode-abi": "^0.7.2",
    "@api3/airnode-node": "^0.7.2",
    "@api3/airnode-ois": "^0.7.2",
    "@api3/airnode-protocol": "^0.8.0",
    "@api3/airnode-protocol-v1": "0.7.0",
    "@ethersproject/experimental": "5.6.0",
    "@mermaid-js/mermaid-cli": "^9.1.6",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@prisma/generator-helper": "^4.3.1",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.183",
    "@types/node": "^17.0.2",
    "@types/prompts": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.5",
    "dotenv": "^16.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-functional": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.4",
    "hardhat": "^2.9.3",
    "hardhat-deploy": "^0.11.4",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "prisma": "^4.4.0",
    "prisma-erd-generator": "^1.2.0",
    "prisma-typegraphql-types-generator": "^1.5.14",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@api3/airnode-admin": "^0.7.2",
    "@api3/airnode-validator": "^0.7.2",
    "@prisma/client": "^4.3.1",
    "graphql-scalars": "^1.18.0",
    "zod": "^3.14.2"
  }
}
